{
    "@context": {
        "@version": 1.1,
        "@protected": true,
        "id": "@id",
        "type": "@type",
        "schema": "https://schema.org/",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "discom": "https://nfh-trust-labs.github.io/vc-schemas/energy-credentials/consumer-profile-vc/context.jsonld#",

        "DiscomConsumerCredential": {
            "@id": "discom:DiscomConsumerCredential",
            "@context": {
                "@version": 1.1,
                "@protected": true,
                "id": "@id",
                "type": "@type",
                "consumerNumber": {
                    "@id": "discom:consumerNumber",
                    "@type": "xsd:string"
                },
                "fullName": {
                    "@id": "schema:name",
                    "@type": "xsd:string"
                },
                "installationAddress": {
                    "@id": "discom:installationAddress",
                    "@type": "@id",
                    "@context": {
                        "fullAddress": {
                            "@id": "schema:streetAddress",
                            "@type": "xsd:string"
                        },
                        "district": {
                            "@id": "discom:district",
                            "@type": "xsd:string"
                        },
                        "pincode": {
                            "@id": "schema:postalCode",
                            "@type": "xsd:string"
                        },
                        "state": {
                            "@id": "schema:addressRegion",
                            "@type": "xsd:string"
                        }
                    }
                },
                "premisesType": {
                    "@id": "discom:premisesType",
                    "@type": "xsd:string"
                },
                "connectionType": {
                    "@id": "discom:connectionType",
                    "@type": "xsd:string"
                },
                "sanctionedLoadKW": {
                    "@id": "discom:sanctionedLoadKW",
                    "@type": "xsd:decimal"
                },
                "tariffCategoryCode": {
                    "@id": "discom:tariffCategoryCode",
                    "@type": "xsd:string"
                },
                "meterNumber": {
                    "@id": "discom:meterNumber",
                    "@type": "xsd:string"
                },
                "serviceConnectionDate": {
                    "@id": "discom:serviceConnectionDate",
                    "@type": "xsd:date"
                },
                "derSolar": {
                    "@id": "discom:derSolar",
                    "@type": "@id",
                    "@context": {
                        "capacityKWp": {
                            "@id": "discom:capacityKWp",
                            "@type": "xsd:decimal"
                        },
                        "commissioningDate": {
                            "@id": "discom:commissioningDate",
                            "@type": "xsd:date"
                        }
                    }
                },
                "derBattery": {
                    "@id": "discom:derBattery",
                    "@type": "@id",
                    "@context": {
                        "storageCapacityKWh": {
                            "@id": "discom:storageCapacityKWh",
                            "@type": "xsd:decimal"
                        },
                        "powerRatingKW": {
                            "@id": "discom:powerRatingKW",
                            "@type": "xsd:decimal"
                        },
                        "commissioningDate": {
                            "@id": "discom:commissioningDate",
                            "@type": "xsd:date"
                        }
                    }
                }
            }
        },

        "DiscomIssuer": {
            "@id": "discom:DiscomIssuer",
            "@context": {
                "@version": 1.1,
                "@protected": true,
                "id": "@id",
                "type": "@type",
                "name": {
                    "@id": "schema:name",
                    "@type": "xsd:string"
                },
                "licenseNumber": {
                    "@id": "discom:licenseNumber",
                    "@type": "xsd:string"
                }
            }
        }
    }
}
