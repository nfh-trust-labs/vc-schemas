                },
                "validFrom": {
                    "@id": "discom:validFrom",
                    "@type": "xsd:date"
                },
                "validUntil": {
                    "@id": "discom:validUntil",
                    "@type": "xsd:date"
                }
            }
        },

        "DiscomIssuer": {
            "@id": "discom:DiscomIssuer",
            "@context": {
                "@version": 1.1,
                "@protected": true,
                "id": "@id",
                "type": "@type",
                "name": {
                    "@id": "schema:name",
                    "@type": "xsd:string"
                },
                "licenseNumber": {
                    "@id": "discom:licenseNumber",
                    "@type": "xsd:string"
                }
            }
        }
    }
